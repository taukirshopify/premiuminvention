
<script src="{{ 'product-form.js' | asset_url }}" defer="defer"></script>
<div class="pro-new-tab" id="pro-new-tab">
<div class="page-width">

<div class="title">
<h2 class="section-header">{{ section.settings.heading }}</h2>
<h6>{{ section.settings.subheading }}</h6>
</div>
 <div class="datss"> 
  {%- for block in section.blocks -%} 
<div class="data-settings wewe">
    {% if block.settings.pro_img !=blank %}
 <img src="{{ block.settings.pro_img | img_url: 'master'  }}">
  {% endif %}
    
{% if block.settings.rating_number !=blank %}
    <div class="star--rating">
    <div class="star--s">
       <div class="loox-rating" data-id="6763404460093" data-rating="4.9" data-raters="82" title="4.9 rating (82 votes)" aria-label="4.9 rating (82 votes)" data-rating-upgraded="1">
          <div class="loox-rating-content">
             <svg class="loox-icon" viewBox="0 0 24 24" data-lx-fill="full">
                <title>4.9 rating ({{block.settings.rating_number}} votes)</title>
                <use href="#looxicons-rating-icon"></use>
             </svg>
             <svg class="loox-icon" viewBox="0 0 24 24" data-lx-fill="full" aria-hidden="true">
                <use href="#looxicons-rating-icon"></use>
             </svg>
             <svg class="loox-icon" viewBox="0 0 24 24" data-lx-fill="full" aria-hidden="true">
                <use href="#looxicons-rating-icon"></use>
             </svg>
             <svg class="loox-icon" viewBox="0 0 24 24" data-lx-fill="full" aria-hidden="true">
                <use href="#looxicons-rating-icon"></use>
             </svg>
             <svg class="loox-icon" viewBox="0 0 24 24" data-lx-fill="full" aria-hidden="true">
                <use href="#looxicons-rating-icon"></use>
             </svg>
             <span class="loox-rating-label">&nbsp;({{ block.settings.rating_number }})</span>
          </div>
       </div>
    </div>
    <div class="rating--nmbrs">4.9 </div>
 </div>
  {% else %}
    <div class="star--rating">
   <a href="#looxReviewsFrame"><div class="loox-rating" data-id="{{ product.id }}" data-rating="{{ product.metafields.loox.avg_rating }}" data-raters="{{ product.metafields.loox.num_reviews }}"></div></a>
    </div>
  {% endif %}
 {% if block.settings.package_name !=blank %}
   <h5>{{ block.settings.package_name }}</h5>
  {% endif %}
 {% if block.settings.product_title !=blank %}
 <h6>{{ block.settings.product_title }} </h6>
      {% endif %}
 {% if block.settings.text !=blank %}
 <div class="rte product_text_custom">{{ block.settings.text }} </div>
      {% endif %}
    <div class="moneeys">
    
 {% if block.settings.sale_price !=blank %}
<p class="sale---price ">
   <span class="money">{{ block.settings.sale_price }}</span>

  {% if block.settings.saving_price !=blank %}
 <h2 class="money"><span class="money">{{ block.settings.saving_price }}</span></h2>
 {% endif %}
    <span class="money1">{{ block.settings.off_price}} Off</span>
 </p>
   {% endif %}
 </div>
{%- assign product_form_id = 'product-form-' | append: block.id -%}
    <div class="buy_now_custom">
        <div class="buynow-btn tab--selction">
          <div class="number">
            <span class="minus">-</span>
            <input aria-label="Quantity" name="quantity" type="number" id="Quantity-{{ block.id }}" form="{{ product_form_id }}" value="1"/>
            <span class="plus">+</span></div>
      <button class="2x" proid="{{ block.settings.product_id }}">PRE-ORDER</button>
    </div>
    <product-form class="product-form">
                  {%- form 'product', product, id: product_form_id, class: 'form', novalidate: 'novalidate', data-type: 'add-to-cart-form' -%}
                    <input type="hidden" name="id" value="{{ block.settings.product_id }}" disabled>
                    <div class="product-form__buttons">
                      <button
                        type="submit"
                        name="add"
                        class="product-form__submit button button--full-width {% if block.settings.show_dynamic_checkout and product.selling_plan_groups == empty %}button--secondary{% else %}button--primary{% endif %}"
                      {% if product.selected_or_first_available_variant.available == false %}disabled{% endif %}
                      >
                          <span>
                            {%- if product.selected_or_first_available_variant.available -%}
                            BUY NOW
                            {%- else -%}
                              BUY NOW
                            {%- endif -%}
                          </span>
                          <div class="loading-overlay__spinner hidden">
                            <svg aria-hidden="true" focusable="false" role="presentation" class="spinner" viewBox="0 0 66 66" xmlns="http://www.w3.org/2000/svg">
                              <circle class="path" fill="none" stroke-width="6" cx="33" cy="33" r="30"></circle>
                            </svg>
                          </div>
                      </button>
                    </div>
                  {%- endform -%}
  </product-form>
</div>




 {% if block.settings.shipday !=blank %}
 <p class="shipday">{{ block.settings.shipday }}</p>
{% else  %}
    {% if request.host contains 'ca.' %}
    <p class="shipday">{{ settings.txt_ca }}</p>
     {% elsif  request.host contains 'au.' %}
      <p class="shipday">{{ settings.txt_au }}</p>
    {% elsif request.host contains 'eu.' %}
    <p class="shipday">{{ settings.txt_eu }}</p>
     {% elsif  request.host contains 'nz.'   %} 
   <p class="shipday">{{ settings.txt_nz }}</p>
  {% elsif request.host contains 'sa.'  %}
<p class="shipday">{{ settings.txt_sa }}</p>

    {% elsif request.host contains 'uk.'  %}
  <p class="shipday">{{ settings.txt_uk }}</p>
     {% else %}
        <p class="shipday">{{ settings.txt_usa }}</p>
{% endif %}
   {% endif %}

 
