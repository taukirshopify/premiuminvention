{% schema %}
{
"name": "new-product--tab-select",
"settings": [
  {
  "type": "text",
  "id": "heading",
  "label": "Top Heading"
},
   {
  "type": "text",
  "id": "subheading",
  "label": "Sub Heading"
}
],
"blocks": [
{
  "type": "text",
  "name": {
    "en": "product"
  },
"settings": [
        {
      "type": "image_picker",
      "id": "pro_img",
      "label":"Product Image"
    },
     {
      "type": "text",
      "id": "package_name",
      "label": "Package Name"
    },
  {
      "type": "text",
      "id": "product_title",
      "label": "Product Name"
    },
     {
      "type": "richtext",
      "id": "text",
      "label": "Product Text"
    },
  {
      "type": "text",
      "id": "product_id",
      "label": "Product Id"
    },
  {
      "type": "text",
      "id": "saving_price",
      "label": "Main Price"
    },
  {
      "type": "text",
      "id": "sale_price",
      "label": "Sale Price"
    },
{
      "type": "text",
      "id": "off_price",
      "label": "Off Price"
    },
 
  {
      "type": "text",
      "id": "shipday",
      "label": "Ship Day"
    },
  {
      "type": "text",
      "id": "rating_number",
      "label": "Rating Number"
    },
  {
      "type": "text",
      "id": "money_back",
      "label": "Money Back Text"
    },
  {
      "type": "text",
      "id": "warranty_text",
      "label": "Warranty Text"
    }
  
]
}
  ],
"presets": [
{
  "name": "new-product--tab-select"
}
]
}
{% endschema %}
<style>

.tab--selction .plus,
.tab--selction .minus {
width: 41px;
height: 41px;
font-size: 17px;
  cursor: pointer;
}
.tab--selction .number {
  margin: 0;
  display: flex;
  align-items: center;
  justify-content: center;
}
.buynow-btn.tab--selction {
  display: flex;
  align-items: center;
  justify-content: left;
  gap: 17px;
}
.tab--selction input {
height: 41px;
width: 51px;
text-align: center;
font-size: 19px;
border-radius: 0;
background: #f8f8f8;
display: inline-block;
vertical-align: middle;
border-left: none;
border-right: none;
border: none;
font-weight: 700;
}
.buynow-btn span.minus:after {
position: absolute;
content: "";
width: 1px;
height: 60%;
right: 0;
background: #ddd;
margin: auto;
}
.tab--selction .minus, .tab--selction .plus {
  background: #f2f2f2;
  border-radius: 0;
  display: inline-block;
  vertical-align: middle;
  text-align: center;
  padding: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
}
@media (max-width:768px) {
  .tab--selction .plus,
.tab--selction .minus {
width: 33px;
height: 33px;
font-size: 16px;
}
  .pro-new-tab button.\32x {
  padding: 10px 30px;
}
}
</style>
<script>
jQuery('.tab--selction .minus').click(function(){
 var quantity =  parseInt(jQuery(this).next().val());
 var quantity =  quantity - 1;
  if(quantity>0){
  jQuery(this).next().val(quantity);
}
  
});
jQuery('.tab--selction .plus').click(function(){
 var quantity =  parseInt(jQuery(this).prev().val());
 var quantity =  quantity + 1;
  jQuery(this).prev().val(quantity);
  
});
</script>