          {%- when 'text' -%}
            <div class="rich-text__text rte" {{ block.shopify_attributes }}>{{ block.settings.text }}</div>
           {%- when 'tab_heading' -%}
            <div class="rich-text__text tab_heading" {{ block.shopify_attributes }}>
