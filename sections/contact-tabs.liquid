                {% if block.settings.heading1 != blank %}
              <div class="tab_heading_item active" data-tab="1">
                <span class="desk-only">{{ block.settings.heading1 }}</span>
                <span class="mob-only">{{ block.settings.heading1_mobile }}</span>
              </div>
                  {% endif %}
                {% if block.settings.heading2 != blank %}
