<script>
  jQuery('.tab_heading_item').click(function(e){
    var active_tab =jQuery(this).attr('data-tab');
    var active_tab ='[data-tab="'+active_tab+'"]';
    if (jQuery(this).hasClass("active")) {
} else {
    jQuery('.tab_heading_item').removeClass('active');
    jQuery(this).addClass('active');
    jQuery('.tab_content').hide();
    jQuery('.tab_content'+active_tab).slideToggle();
    }
});
  jQuery('.tab_content_collapse_trigger').click(function(e){
    jQuery(this).parents('.tab_content_collapse').toggleClass('active');
    jQuery(this).next().slideToggle();
});

  
  jQuery('.tab_content_tab_item').click(function(e){
    var active_tab =jQuery(this).attr('data-tab');
    var active_tab ='[data-tab="'+active_tab+'"]';
    if (jQuery(this).hasClass("active")) {
} else {
    jQuery(this).parents('.tab_content_flex').find('.tab_content_tab_item').removeClass('active');
    jQuery(this).addClass('active');
    jQuery(this).parents('.tab_content_flex').find('.tab_content_tabs_content_item').hide();
    jQuery(this).parents('.tab_content_flex').find('.tab_content_tabs_content_item'+active_tab).slideToggle();
    }
});
</script>
